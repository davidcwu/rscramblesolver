#!/usr/bin/env ruby

begin
  require 'rscramblesolver/rscramblesolver'
rescue LoadError
  require 'rubygems'
  require 'rscramblesolver/rscramblesolver'
end

include Rscramblesolver

board_creator = BoardCreator.new
board = board_creator.create_board

search = Search.new(
    board: board
  )

search_results = search.execute
puts search_results